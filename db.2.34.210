;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	cotillas.es. root.cotillas.es. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	cotillas.es.
2	IN	PTR	ns1.cotillas.es.
11	IN	PTR	windows.cotillas.es

